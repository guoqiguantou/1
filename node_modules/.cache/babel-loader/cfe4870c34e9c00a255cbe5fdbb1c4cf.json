AsCC,kBAAAA,MAAM,EAANA;AAAtC,iBAArB;AACD;;AANkC,iDAQ5BI,IAR4B;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASpC;AA9Ia,CAAhB;AAiJA,eAAe;AACbmE,EAAAA,UAAU,EAAE,IADC;AAEbzE,EAAAA,KAAK,EAALA,KAFa;AAGbI,EAAAA,SAAS,EAATA,SAHa;AAIbG,EAAAA,OAAO,EAAPA;AAJa,CAAf","sourcesContent":["import User from '../api/user';\r\nimport StorageUtil from '@/utils';\r\nimport {authPathMap, noAuthPathArr} from '@/router/menu';\r\nimport _ from 'lodash';\r\n\r\nconst state = {\r\n  token: '',\r\n  userId: '',\r\n  userInfo: {},\r\n};\r\nconst mutations = {\r\n  SET_USER_INFO: (state, data) => {\r\n    state.token = data.token;\r\n    state.userId = data.userId;\r\n    state.userInfo = data;\r\n  },\r\n};\r\nconst actions = {\r\n  //获取登陆验证码    /sms/captcha/send/\r\n  async getCode ({commit}, data) {\r\n    const result = await User.getCodeApi (data);\r\n    return result;\r\n  },\r\n  //短信登陆\r\n  async loginSms ({commit}, params) {\r\n    const result = await User.loginSmsApi (params);\r\n    if (result.data) {\r\n      const data = result.data;\r\n      StorageUtil.setToken (data.token);\r\n      StorageUtil.setAuth (data);\r\n      commit ('SET_USER_INFO', data);\r\n    }\r\n    return result;\r\n  },\r\n  //获取用户token\r\n  async loginRegister ({commit}, params) {\r\n    const result = await User.loginRegisterApi (params);\r\n    if (result.data) {\r\n      const data = result.data;\r\n      StorageUtil.setToken (data.token);\r\n      StorageUtil.setAuth (data);\r\n      commit ('SET_USER_INFO', data);\r\n    }\r\n    return result;\r\n  },\r\n\r\n  //密码登录\r\n  async loginPass ({commit}, params) {\r\n    const result = await User.loginPassApi (params);\r\n    if (result.data) {\r\n      const data = result.data;\r\n      data.roleId = data.roleList[0].roleId;\r\n      StorageUtil.setToken (data.token);\r\n      StorageUtil.setAuth (data);\r\n      commit ('SET_USER_INFO', data);\r\n    }\r\n    return result;\r\n  },\r\n  //修改密码\r\n  async updatePass ({commit}, params) {\r\n    const data = await User.updatePassApi (params);\r\n    return data;\r\n  },\r\n  //创建用户\r\n  async createUser ({commit}, params) {\r\n    const data = await User.createUserApi (params);\r\n    return data;\r\n  },\r\n  //禁用用户\r\n  async banUser ({commit}, params) {\r\n    const data = await User.banUserApi (params);\r\n    return data;\r\n  },\r\n  //启用用户\r\n  async passUser ({commit}, params) {\r\n    const data = await User.passUserApi (params);\r\n    return data;\r\n  },\r\n  // 分页查询用户列表（查询所有用户）\r\n  async getUserList ({commit}, params) {\r\n    const data = await User.getUserListApi (params);\r\n    return data;\r\n  },\r\n  // 分页查询店铺用户列表（查询店铺用户）\r\n  async getShopUserList ({commit}, params) {\r\n    const data = await User.getShopUserListApi (params);\r\n    return data;\r\n  },\r\n  // 获取用户详情\r\n  async getUserDetail ({commit}, params) {\r\n    const data = await User.getUserDetailApi (params);\r\n    return data;\r\n  },\r\n  //  删除用户地址\r\n  async delUserAddress ({commit}, params) {\r\n    const data = await User.delUserAddressApi (params);\r\n    return data;\r\n  },\r\n  //  注册创建商户角色\r\n  async createRole ({commit}, params) {\r\n    const data = await User.createRoleApi (params);\r\n    return data;\r\n  },\r\n  //查询角色申请信息\r\n  async roleMsg ({commit}, params) {\r\n    const data = await User.RoleMsgApi (params);\r\n    return data;\r\n  },\r\n\r\n  //获取用户权限\r\n  async getUserMenuList ({commit}, params) {\r\n    const data = await User.getUserMenuListApi (params);\r\n    let authArray = data.data.menuIdList || [];\r\n    let path = []; //获取路由\r\n    authArray.forEach (item => {\r\n      if (authPathMap.has (item)) path.push (authPathMap.get (item));\r\n    });\r\n    path = path.concat (noAuthPathArr);\r\n    StorageUtil.setPath (path[0]);\r\n    StorageUtil.setAuthId (authArray);\r\n    return {data, homePath: path[0]};\r\n  },\r\n  //编辑用户账户\r\n  async getUpdatePaymentInfo ({commit}, params) {\r\n    const data = await User.getUpdatePaymentInfoApi (params);\r\n    return data;\r\n  },\r\n  //编辑用户账户\r\n  async getAddPaymentInfo ({commit}, params) {\r\n    const data = await User.getAddPaymentInfoApi (params);\r\n    return data;\r\n  },\r\n  //编辑用户信息\r\n  async getEditUser ({commit,dispatch}, params) {\r\n    const data = await User.getEditUserApi (params);\r\n    if(data.retCode==200){\r\n      return dispatch('getUser')\r\n    }else{\r\n      return data\r\n    }\r\n  },\r\n  //获取用户信息\r\n  async getUser ({commit}, params) {\r\n    const data = await User.getUserApi (params);\r\n    const {avatar,envState,nickname,phone,token,userId}=data.data\r\n    commit ('SET_USER_INFO', {avatar,envState,nickname,phone,token,userId});\r\n    StorageUtil.setAuth ({avatar,envState,nickname,phone,token,userId});\r\n    return data;\r\n  },\r\n  //修改手机号\r\n  async changePhone ({commit}, params) {\r\n    const data = await User.changePhoneApi (params);\r\n    if(data.retCode==200){\r\n      const {avatar,envState,nickname,phone,token,userId}=data\r\n      commit ('SET_USER_INFO', {avatar,envState,nickname,phone,token,userId});\r\n      StorageUtil.setAuth ({avatar,envState,nickname,phone,token,userId});\r\n    }\r\n    \r\n    return data\r\n  },\r\n};\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions,\r\n};\r\n"]}]}