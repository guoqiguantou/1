n    return data;\r\n  },\r\n  //禁用店铺\r\n  async createBan({ commit }, params) {\r\n    const data = await Shop.createBanApi(params);\r\n    return data;\r\n  },\r\n  //启用店铺\r\n  async createPass({ commit }, params) {\r\n    const data = await Shop.createPassApi(params);\r\n    return data;\r\n  },\r\n  // 商品上架\r\n  async goodsToBan({ dispatch, commit }, params) {\r\n    const { data, retCode, retMsg } = await Shop.goodsToBanApi(params);\r\n    if (retCode == 200 && data) {\r\n      commit('changeNum', 1);\r\n      dispatch('getShopList');\r\n      return data;\r\n    } else {\r\n      return retMsg\r\n    }\r\n  },\r\n  // 商品下架\r\n  async goodsToPass({ dispatch, commit }, params) {\r\n    const { data, retCode, retMsg } = await Shop.goodsToPassApi(params);\r\n    if (retCode == 200 && data) {\r\n      commit('changeNum', 1);\r\n      dispatch('getShopList');\r\n      return data;\r\n    } else {\r\n      return retMsg\r\n    }\r\n\r\n  },\r\n  // 商品创建\r\n  async createShop({ commit }, params) {\r\n    const data = await Shop.createShopApi(params);\r\n    return data;\r\n  },\r\n  // 查询商品详情\r\n  async getShopDetail({ commit }, params) {\r\n    const data = await Shop.getShopDetailApi(params);\r\n    return data;\r\n  },\r\n\r\n  // 商品暂存\r\n  async shopTemporary({ commit }, params) {\r\n    const data = await Shop.shopTemporaryApi(params);\r\n    return data;\r\n  },\r\n  // 保存商品编辑\r\n  async saveGoods({ commit }, params) {\r\n    const data = await Shop.saveGoodsApi(params);\r\n    return data;\r\n  },\r\n\r\n  //  查询品牌\r\n  async initBrand({ commit }, params) {\r\n    const data = await Shop.getBrandListApi(params);\r\n    return data;\r\n  },\r\n  //  查询类目\r\n  async getCategoryList() {\r\n    const data = await Shop.getCategoryListApi({});\r\n    return data;\r\n  },\r\n  //  查询类目属性\r\n  async getCategoryValue({ commit }, params) {\r\n    const data = await Shop.getCategoryValueApi(params);\r\n    return data;\r\n  },\r\n  // 查询所有商品\r\n  async allShopList({ commit }, params) {\r\n    const data = await Shop.getAllShopListApi(params);\r\n    return data;\r\n  },\r\n  //修改num分页\r\n  async changeNumHandle({ dispatch, commit }, params) {\r\n    commit('changeNum', params);\r\n    dispatch('getShopList');\r\n  },\r\n  async changeSizeHandle({ dispatch, commit }, params) {\r\n    commit('changeSize', params);\r\n    dispatch('getShopList');\r\n  },\r\n  // 搜索\r\n  async changeFilterHandle({ dispatch, commit }, params) {\r\n    commit('changeFilter', params);\r\n    commit('changeNum', 1);\r\n    dispatch('getShopList');\r\n  },\r\n  //分页查询商品列表\r\n  async getShopList({ state, commit },) {\r\n    commit('listData', {})\r\n    let params = {\r\n      pageSize: state.pageSize,\r\n      pageNum: state.pageNum - 1,\r\n      ...state.fliter,\r\n    }\r\n    const { data, retCode } = await Shop.getShopListApi(params);\r\n    if (retCode === 200 && data) commit('listData', data)\r\n    return data;\r\n  },\r\n  // 提交询价\r\n  async commitItemPrice({ commit }, params) {\r\n    const data = await Shop.commitPriceApi(params);\r\n    return data;\r\n  },\r\n\r\n\r\n};\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions,\r\n};\r\n"]}]}