arts实例\n      let myChart = this.$echarts.init(document.getElementById(\"myChart5\"));\n      //   绘制图表\n      myChart.setOption({\n        color: EChartColorConfig,\n        tooltip: {\n          trigger: \"item\",\n        },\n        grid: {\n          containLabel: true,\n          top: 10,\n          bottom: 0,\n          left: 10,\n          right: 10,\n        },\n        series: [\n          {\n            name: \"批量授权\",\n            type: \"pie\",\n            radius: [\"60%\", \"90%\"],\n            itemStyle: {\n              //   borderRadius: 5,\n              //   borderColor: \"#fff\",\n              //   borderWidth: 1,\n            },\n            label: {\n              show: true,\n              formatter: \"{c}%\\n{b}\",\n              align: \"center\",\n              width: 80,\n              overflow: \"truncate\",\n            },\n            emphasis: {\n              label: {\n                show: true,\n                fontWeight: \"bold\",\n              },\n            },\n            labelLine: {\n              show: true,\n            },\n            data: [\n              { value: 38, name: \"未授权\" },\n              { value: 23, name: \"Windows\" },\n              { value: 18, name: \"Office\" },\n              { value: 16, name: \"Windows Server\" },\n              { value: 5, name: \"Sql Server\" },\n            ],\n          },\n        ],\n      });\n    },\n  },\n};\n</script>\n<style scoped lang=\"less\">\n#myChart5 {\n  width: 100%;\n  height: 100%;\n}\n</style>"]}]}